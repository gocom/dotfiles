# Connection timeout.
connect-timeout = 30

# File to store cookies.
cookie-jar = ~/.curl-cookies

# Follow redirects.
location

# Maximum number of redirects to follow.
max-redirs = 30

# Default wait time between requests to save servers.
retry = 10

# Wait between retries.
retry-delay = 15

# Fedoras off and set user-agent to appear like we a real browser.
user-agent = "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/57.0.2987.133 Safari/537.36"
